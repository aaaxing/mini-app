@import '../../weui.wxss'

.wx-mask{
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.3);
}

.head-picker-box{
    height:70rpx;
    width:187.5rpx;
    position:relative;
    float:left;
    z-index:5000;
    background:white;
}


.pickers{
    width:100%;
    height:100%;
    text-align:center;
    line-height:70rpx;
    z-index:5001
}

.cone{
    color:#539FDD
}
.ctwo{
    color:gray
}

.pos1,.pos2,.pos3,.pos4{
    position:absolute;
    top:70rpx;
    z-index:5000;
}

.pos1{
    left:0;
}

.pos2{
    left:-187.5rpx;
}

.pos3{
    left:-375rpx
}

.pos4{
    left:-562.5rpx
}

.check{
    width:30rpx;
    height:30rpx;
    position:absolute;
    top:20rpx;
    left:30rpx;
}

